@page
@model WebApplication1.Pages.AuditLogsModel
@{
    ViewData["Title"] = "Audit Logs";
}

<h2>Audit Logs</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Timestamp (UTC)</th>
            <th>UserId</th>
            <th>Action</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var a in Model.Logs)
        {
            <tr>
                <td>@a.Timestamp.ToString("u")</td>
                <td>@a.UserId</td>
                <td>@a.Action</td>
                <td>@a.Details</td>
            </tr>
        }
    </tbody>
</table>
